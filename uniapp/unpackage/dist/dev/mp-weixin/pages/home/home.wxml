<view data-event-opts="{{[['tap',[['cancelPhotoChange',['$event']]]]]}}" style="height:800px;" bindtap="__e"><view class="personalInfo"><view><image class="user_photo avatar-border" src="{{avatarUrl}}" data-event-opts="{{[['tap',[['toggleDropdown',['$event']]]]]}}" bindtap="__e"></image></view><view hidden="{{!(showDropdown)}}" class="dropdown"><text data-event-opts="{{[['tap',[['changeAvatar',['$event']]]]]}}" class="option" bindtap="__e">更改头像</text></view><view style="margin-top:30px;margin-left:20px;font-size:25px;">{{"用户："+username}}</view></view><view class="icon-container"><view data-event-opts="{{[['tap',[['goToMyOrders',['$event']]]]]}}" class="icon-item" bindtap="__e"><image class="icon" src="../../static/order.png"></image><text class="icon-label">我的订单</text></view><view data-event-opts="{{[['tap',[['goToMyCollections',['$event']]]]]}}" class="icon-item" bindtap="__e"><image class="icon" src="../../static/mark.png"></image><text class="icon-label">我的收藏</text></view><view data-event-opts="{{[['tap',[['goToMyPosts',['$event']]]]]}}" class="icon-item" bindtap="__e"><image class="icon" src="../../static/myPost.png"></image><text class="icon-label">我的帖子</text></view><view data-event-opts="{{[['tap',[['goToMyPets',['$event']]]]]}}" class="icon-item" bindtap="__e"><image class="icon" src="../../static/animal.png"></image><text class="icon-label">我的宠物</text></view></view></view>