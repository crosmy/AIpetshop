<view><view><text>请填入宠物姓名*</text><input type="text" placeholder="宠物姓名" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['form']]]]]}}" value="{{form.name}}" bindinput="__e"/><text>请填入种类（如猫、狗）</text><input type="text" placeholder="宠物种类" data-event-opts="{{[['input',[['__set_model',['$0','type','$event',[]],['form']]]]]}}" value="{{form.type}}" bindinput="__e"/><text>请填入具体品种（波斯猫，金毛犬）</text><input type="text" placeholder="宠物品种" data-event-opts="{{[['input',[['__set_model',['$0','breed','$event',[]],['form']]]]]}}" value="{{form.breed}}" bindinput="__e"/><text>请填入宠物年龄</text><input type="text" placeholder="宠物年龄" data-event-opts="{{[['input',[['__set_model',['$0','age','$event',[]],['form']]]]]}}" value="{{form.age}}" bindinput="__e"/><text>请填入具体描述</text><input type="text" placeholder="宠物描述" data-event-opts="{{[['input',[['__set_model',['$0','description','$event',[]],['form']]]]]}}" value="{{form.description}}" bindinput="__e"/><text>请填入宠物健康状况</text><input type="text" placeholder="宠物状况" data-event-opts="{{[['input',[['__set_model',['$0','healthInformation','$event',[]],['form']]]]]}}" value="{{form.healthInformation}}" bindinput="__e"/><text>请填入宠物接种疫苗情况</text><input type="text" placeholder="宠物疫苗" data-event-opts="{{[['input',[['__set_model',['$0','vaccinations','$event',[]],['form']]]]]}}" value="{{form.vaccinations}}" bindinput="__e"/><text>请填入宠物价格</text><input type="text" placeholder="宠物价格" data-event-opts="{{[['input',[['__set_model',['$0','price','$event',[]],['form']]]]]}}" value="{{form.price}}" bindinput="__e"/><text>请填入宠物图片Url</text><input placeholder="宠物url" data-event-opts="{{[['input',[['__set_model',['','inputPhotoUrl','$event',[]]]]]]}}" value="{{inputPhotoUrl}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['imageUpload',['$event']]]]]}}" bindtap="__e">上传图片</button></view><view><image src="{{petPhotoUrl}}" mode="aspectFit"></image></view><button data-event-opts="{{[['tap',[['createPet',['$event']]]]]}}" class="fab-button " bindtap="__e">创建</button></view>